
<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Event name</th>
      <th>Event location</th>
      <th>Event date</th>
      <th>User</th>
      <th>Express Interest</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.event_location %></td>
        <td><%= event.event_date %></td>
        <td><%= event.user_id %></td>
        <td><%= event.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% if user_signed_in? %>
  <%= link_to 'New Event', new_event_path %>
<% end %>